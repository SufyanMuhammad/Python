numbers = list(range(0,10))
for number in numbers:
    if number == 1:
        print(f"{number}st")
    elif number == 2:
        print(f"{number}nd")
    elif number == 3:
        print(f"{number}rd")
    else:
        print(f"{number}th")




current_names = ['Sufyan','Daud','Noor','Idrees','Safia']
new_names = ['Sumayyah','Shahid','Fayaaz','Ahmed','Idrees']
for new_name in new_names:
  if new_name in current_names:
    print(f"Hello {new_name} the name you gave is used please give another name.")
  else:
    print(f"Hello {new_name} your this name will be printed out.")





