alien_0 = {'colour': 'Green', 'health': 100, 'points': 15, 'numbers': 500}
alien_1 = {'colour': 'Yellow', 'health': 200, 'points': 30, 'numbers': 450}
alien_2 = {'colour': 'Pink', 'health': 300, 'points': 45, 'numbers': 400}
alien_3 = {'colour': 'Red', 'health': 400, 'points': 60, 'numbers': 350}
alien_4 = {'colour': 'Grey', 'health': 500, 'points': 75, 'numbers': 300}
alien_5 = {'colour': 'Black', 'health': 600, 'points': 90, 'numbers': 250}
alien_6 = {'colour': 'White', 'health': 700, 'points': 105, 'numbers': 200}
alien_7 = {'colour': 'Blue', 'health': 800, 'points': 120, 'numbers': 150}
alien_8 = {'colour': 'Silver', 'health': 900, 'points': 135, 'numbers': 100}
alien_9 = {'colour': 'Gold', 'health': 1000, 'points': 150, 'numbers': 50}

aliens = [alien_0, alien_1, alien_2, alien_3, alien_4, alien_5, alien_6, alien_7, alien_8, alien_9]

for alien in aliens:
    print(f"There are {alien['numbers']} {alien['colour']} aliens which provide " +
          f"{alien['points']} points and have a health of {alien['health']}.\n")





cities = {
    'new york': {'country':'United States', 'population':8.4, 'fact':'more than 800 languages are spoken in NYC'},
    'london': {'country':'United Kingdom', 'population':8.9, 'fact':'capital of the UK'},
    'berlin': {'country':'Germany', 'population':3.6, 'fact':'was separated by the Berlin Wall'},
}
for city, details in cities.items():
    print(f"{city.title()} is located in {details['country']}, has a population of ~{details['population']} million.")
    print(f"Fact about {city.title()}: {details['fact']}\n")





favourite_numbers = {
    'Sufyan': [3, 5, 7, 9],
    'Idrees': [2, 4, 6, 8],
    'Noor':[1, 3, 5, 7, 11]
}
for name, numbers in favourite_numbers.items():
  print(f"{name.title()}'s favourite numbers are:")
  for number in numbers:
    print(f"\t{number}.")
  print("\n")




favourite_places = {
    'Sufyan': ['Saudiarabia','Islamabad'],
    'Idrees': ['Saudiarabia'],
    'Noor': ['Deri','Islamabad','Saudiarabia'],
}
for name, places in favourite_places.items():
  print(f"{name.title()}'s favourite places are:")
  for place in places:
    print(f"\t{place.title()}.")
  print(f"\n")





pet_0 = {'animal':'cat','owner':'Siam'}
pet_1 = {'animal':'cow','owner':'Liala'}
pets = [pet_0,pet_1]
for pet in pets:
  print(f"The animal name is {pet['animal']} and the owner is {pet['owner']}.")





person_0 = {'first name':'Sufyan','last name':'Idrees','age':14,'location':'F18'}
person_1 = {'first name':'Idrees','last name':'Noor','age':40,'location':'F18'}
person_2 = {'first name':'Noor','last name':'Muhammad','age':70,'location':'F18'}

people = [person_0,person_1,person_2]

for person in people:
  for k, v in person.items():
    print(f"{k}: {v}")
  print("\n")





people = {
    'SufyanIdrees': {
        'first name': 'Sufyan',
        'last name': 'Idrees',
        'location': 'F18',
    },
    'IdreesNoor': {
        'first name': 'Idrees',
        'last name': 'Noor',
        'location': 'Dog Hamida',
    },
    'NoorMuhammad': {
        'first name': 'Noor',
        'last name': 'Muhammad',
        'location': 'Malakand',
    },
}

for username, user_info in people.items():
    print(f"\nUsername: {username}.")
    full_name = f"{user_info['first name']} {user_info['last name']}"
    location = user_info['location']
    print(f"\nFull name: {full_name.title()}")
    print(f"\nLocation: {location.title()}")





users = {
    'SufyanIdrees':{
        'first':'Sufyan',
        'last':'Idrees',
        'location':'F18',
    },
    'IdreesNoor':{
        'first':'Idrees',
        'last':'Noor',
        'location':'F18',  
    },
}
for username, user_info in users.items():
    print(f"\nUsername: {username}.")
    full_name = f"{user_info['first']} {user_info['last']}"
    location = user_info['location']   
    print(f"\nFull name: {full_name.title()}")
    print(f"\nLocation: {location.title()}")







pizza = {
    'crust':'thick',
    'toppings':['mushrooms','extra cheese'],
}
print(f"You ordered a {pizza['crust']}-crust pizza with the following toppings:")
for topping in pizza ['toppings']:
  print(f"\t{topping}")






aliens = []
for alien_number in range(15):
  new_alien = {'colour':'green','points':'15','speed':'medium'}
  aliens.append(new_alien)
for alien in aliens[:15]:
  if alien['colour'] == 'green':
    alien['colour'] = 'blue'
    alien['points'] = 30
    alien['speed'] = 'fast'
for alien in aliens[:30]:
  print(aliens)
print(f"Total number of aliens {len(aliens)}.")







aliens = []
for alien_number in range(30):
  new_alien = {'colour':'green','points':'15','speed':'slow'}
  aliens.append(new_alien)
for alien in aliens[:30]:
  print(aliens)
print(f"Total number of aliens {len(aliens)}.")






