favourite_languages = {
    'Sufyan': 'English',
    'Daud': 'Arabic',
    'Noor': 'Pashto',
    'Idrees': 'Arabic',
    'Rayyan': 'Pashto'
}
people = ['Sufyan','Idrees','Daud','Noor','Rayyan','Shaima','Hasna']
for person in people:
  if person in favourite_languages.keys():
    print(f"Hey, {person.title()}, thankyou for coming to the party.")
  else:
    print(f"Hey, {person.title()}, may i inquire as to why you were unable to attend the party.")






rivers = {
    'Indus': 'Pakistan',
    'Nile': 'Egypt',
    'Amazon': 'Brazil',
    'Yangtse': 'China',
}

for river, country in rivers.items():
    print(f"The {river.title()} runs through {country}.")

print("\n")

for river in rivers.keys():
  print(river.title())
for country in rivers.values():
  print(country.title())

