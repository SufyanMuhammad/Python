print(f"Hello, your position is {alien_0['x_position']}.")

if alien_0['speed'] == 'Slow':
    x_increment = 1
elif alien_0['speed'] == 'Medium':
    x_increment = 2
else:
    x_increment = 3

alien_0['x_position'] = int(alien_0['x_position']) + x_increment
print(f"New position: {alien_0['x_position']}.")

